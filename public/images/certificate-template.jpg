<?xml version="1.0" encoding="UTF-8"?>
<svg xmlns="http://www.w3.org/2000/svg" width="800" height="600" viewBox="0 0 800 600">
  <!-- Background -->
  <rect width="800" height="600" fill="#f5f5f5"/>
  
  <!-- Border -->
  <rect x="20" y="20" width="760" height="560" fill="none" stroke="#333" stroke-width="2"/>
  
  <!-- Header -->
  <text x="400" y="80" font-family="Arial, sans-serif" font-size="36" text-anchor="middle" fill="#333">
    CERTIFICATE OF AUTHORITY
  </text>
  
  <!-- Subheader -->
  <text x="400" y="130" font-family="Arial, sans-serif" font-size="20" text-anchor="middle" fill="#666">
    This certifies that
  </text>
  
  <!-- Placeholder for franchise name -->
  <text x="400" y="250" font-family="Arial, sans-serif" font-size="32" text-anchor="middle" fill="#000" font-weight="bold">
    [FRANCHISE NAME]
  </text>
  
  <!-- Description -->
  <text x="400" y="320" font-family="Arial, sans-serif" font-size="18" text-anchor="middle" fill="#333">
    has been authorized as a franchise partner of
  </text>
  
  <!-- Company Name -->
  <text x="400" y="360" font-family="Arial, sans-serif" font-size="24" text-anchor="middle" fill="#000" font-weight="bold">
    CreditDost
  </text>
  
  <!-- Validity -->
  <text x="400" y="420" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#666">
    This certificate is valid indefinitely
  </text>
  
  <!-- Signature line -->
  <line x1="550" y1="500" x2="750" y2="500" stroke="#333" stroke-width="1"/>
  <text x="650" y="520" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#333">
    Authorized Signature
  </text>
  
  <!-- Date -->
  <text x="50" y="520" font-family="Arial, sans-serif" font-size="14" fill="#333">
    Date: [DATE]
  </text>
  
  <!-- Certificate ID -->
  <text x="50" y="550" font-family="Arial, sans-serif" font-size="12" fill="#666">
    Certificate ID: [CERT_ID]
  </text>
</svg>